#begin CPP

PROJECT_LOC = "buggy-" VER
TESTCASE_LOC = PROJ "-buggy-" VER "-" CASE

COVERAGE_NAME = "summary.json"

TEST_NAME = CASE ".test"
TEST_PASS = "passed"

#end

#begin PYTHON

PROJECT_LOC = PROJ "-" VER "/" PROJ
TESTCASE_LOC = PROJ "-" VER "/" PROJ "/coverage/" CASE

COVERAGE_NAME = "summary.json"

TEST_NAME = CASE ".test"
TEST_PASS = "passed"

#end

